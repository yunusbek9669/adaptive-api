{
    "name": "yunusbek/adaptive-api",
    "description": "Adaptive API package for Yii2 projects",
    "type": "library",
    "license": "MIT",
    "require": {
        "yiisoft/yii2": "^2.0",
        "greenlion/php-sql-parser": "^4.7",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "Yunusbek\\AdaptiveApi\\": "src/"
        }
    },
    "authors": [
        {
            "name": "yunusbek",
            "email": "inomovyunusbek@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
